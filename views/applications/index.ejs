<!-- views/applications/index.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home Page</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Students List</h1>
    <a href="/users/<%=user._id%>/school/new">Add a New Student</a>
    <ul>

      <% school.forEach((school)=>{ %>
        <li><%= school.name %> from <%= school.school %></li>
        <% }) %>

    </ul>
  </body>
</html>
